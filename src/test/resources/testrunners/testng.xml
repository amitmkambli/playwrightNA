<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open Cart Test Regression Suite" thread-count="2" parallel="tests"
	verbose="4">
	<listeners>
		<listener class-name="listeners.ExtentReportListener"></listener>
	</listeners>
	<test name="Open Cart Home page Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.HomePageTests" />
		</classes>
	</test>
	<test name="Open Cart Login page Test">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="tests.LoginPageTest" />
		</classes>
	</test>
</suite>
